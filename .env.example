# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

DATABASE_URL="postgresql://postgres:postgres@localhost:5432/airbnb_clone?schema=public"
GOOGLE_CLIENT_ID=value
GOOGLE_CLIENT_SECRET=value
FACEBOOK_ID=value
FACEBOOK_SECRET=value
NEXTAUTH_SECRET="anything"
NEXTAUTH_URL="http://localhost:3000"
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME="value"
NEXT_PUBLIC_LOOKUP_KEY="value"

# Set to "true" to block mutating API routes during migration cutover.
DB_MIGRATION_READ_ONLY="false"

# Used only by one-time migration/verification scripts.
MONGODB_MIGRATION_URI="mongodb://127.0.0.1:27017/airbnb-clone"
MONGODB_MIGRATION_DB="airbnb-clone"
